{
  "name": "shopping_list",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.1"
  },
  "description": "Shopping list with MERN Stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Natalie Schennum",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "formidable": "^1.2.1",
    "fs": "0.0.1-security",
    "gridfs": "^1.0.0",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.4.19",
    "mongoose-types": "^1.0.3",
    "multer": "^1.4.1",
    "react": "^16.8.4",
    "router": "^1.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
